<% if can? :read, Billing::Subscription.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :billing_subscriptions]),
    label: t('billing/subscriptions.navigation.label'),
  } do |p| %>
    <% p.content_for :icon do %>
      <i class="<%= t('billing/subscriptions.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
